public List<IncidentEntity> getIncidentsDeclaredByClientIgg(String igg) {
    return incidentRepository.findByClient_Igg(igg);
}

public List<IncidentEntity> getIncidentsResolvedByCoeDevIgg(String igg) {
    return incidentRepository.findByCoeDev_Igg(igg);
}
List<IncidentEntity> findByClient_Igg(String igg);

List<IncidentEntity> findByCoeDev_Igg(String igg);
