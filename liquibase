public interface IncidentRepository extends JpaRepository<IncidentEntity, Integer> {
    
    List<IncidentEntity> findByClient_Urn_Value(String clientId);
    
    List<IncidentEntity> findByCoeDev_Urn_Value(String coeDevId);
}
public List<IncidentEntity> getIncidentsResolvedByCoeDev(String userId) {
    return incidentRepository.findByCoeDev_Urn_Value(userId);
}
public List<IncidentEntity> getIncidentsDeclaredByClient(String userId) {
    return incidentRepository.findByClient_Urn_Value(userId);
}
